# Enter your code here. Read input from STDIN. Print output to STDOUT
n,m = list(map(int, input().split()))
List = list(map(int, input().split()))

A = set(map(int, input().split()))
B = set(map(int, input().split()))
Happines = 0

for i in List:
    
    if i in A:
        Happines += 1
        
    elif i in B:
        Happines -= 1
        
print (Happines)
        
    
